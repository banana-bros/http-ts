{
  "name": "@alkocats/http-ts",
  "version": "0.6.4",
  "description": "A Typescript HTTP server wrapper with express.",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "http": "^0.0.0",
    "https": "^1.0.0",
    "jsonwebtoken": "^9.0.0",
    "request": "^2.88.2",
    "rxjs": "^6.6.6",
    "url": "^0.11.0",
    "winston": "^3.3.3",
    "ws": "^7.4.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alkocats/http-ts.git"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/chai": "^4.2.15",
    "@types/express": "^4.17.11",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mocha": "^8.2.1",
    "@types/node": "^14.14.34",
    "@types/winston": "^2.4.4",
    "chai": "^4.3.4",
    "coveralls": "^3.1.0",
    "mocha": "^8.3.2",
    "nodemon": "^2.0.7",
    "nyc": "^15.1.0",
    "ts-node": "9.1.1",
    "tsconfig-lint": "^0.12.0",
    "tslint": "^6.1.3",
    "typedoc": "^0.20.32",
    "typescript": "4.2.3"
  },
  "scripts": {
    "build": "tsc && npm run lint",
    "dev": "tsc --watch & nodemon dev",
    "test": "mocha --require ts-node/register spec/**/*.spec.ts",
    "lint": "tslint --project tslint.json 'src/**/*.ts'",
    "tsc": "tsc",
    "start": "nodemon --config nodemon.json",
    "cover": "nyc  -r lcov -e .ts -x \"**/*.spec.ts\" mocha --require ts-node/register spec/**/*.spec.ts"
  },
  "keywords": [
    "alkocats",
    "node",
    "typescript",
    "express",
    "http"
  ],
